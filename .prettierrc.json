{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "crlf",
  "quoteProps": "consistent",
  "singleAttributePerLine": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "singleQuote": true,
  "jsxSingleQuote": true
}
